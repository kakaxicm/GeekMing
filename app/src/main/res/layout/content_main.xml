<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp">

    <Button
        android:id="@+id/common_adapter_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Common Adapter Entry" />

    <Button
        android:id="@+id/download_manager_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Download Manager Entry" />

    <Button
        android:id="@+id/custom_viewgroup_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Custom ViewGroup Entry" />

</LinearLayout>
